<form class="well" action="/divisions/<%= @division.id() %>" method="post">
  <input name="_method" type="hidden" value="patch">
  <div class="form-group">
    <label for="name">Edit Division Name </label>
    <input id = "name" type="text" name="name" value="<%=@division.name%>" class="form-control">
  </div>
  <button type="submit" class="btn btn-success">Update Division</button>
</form>

<a href="/" class="btn btn-default active" role="button">Home</a>
